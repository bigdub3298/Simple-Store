<%- include('../includes/head.ejs')%> <%-
include('../includes/navigation.ejs')%>

<main>
  <ul class="cart__item-list">
    <% products.forEach(p => { %>
    <li class="cart__item">
      <h1><%= p.title %></h1>
      <h2>Quantity: <%= p.cartItem.quantity %></h2>
    </li>
    <% }) %>
  </ul>
  <div class="centered">
    <h2>Total: $<%= totalSum %></h2>
  </div>
</main>

<%- include('../includes/end.ejs')%>
